<div class="login-image z-depth-5">
    <div class="row valign-wrapper">
        <div class="col l6 m8 s12" id="profileContainer">
            <h3 class="center">Users</h3>
            {{#each users}}
            <div class=“card” id="userCard">
                <div class="center">
                    <strong>Username</strong>: {{this.username }}<br>
                    <strong>First Name</strong>: {{ this.user_first_name }}<br>
                    <strong>Last Name</strong>: {{ this.user_last_name }}<br>
                    <strong>Phone Number</strong>: {{ this.user_number }}<br>
                    <strong>Email</strong>: {{ this.user_email }}<br>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>
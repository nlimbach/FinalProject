<div class="login-image z-depth-5">
    <div class="container z-depth-4" id="inventoryContainer">
        <table class="highlight centered" id="inventoryTable">
            <thead>
            <tr>
                <th>Username</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Phone Number</th>
                <th>Email</th>
            </tr>
            </thead>
            <tbody>
            {{#each users}}
                <tr>
                    <td>{{this.username}}</td>
                    <td>{{this.user_first_name}}</td>
                    <td>{{this.user_last_name}}</td>
                    <td>{{this.user_number }}</td>
                    <td>{{this.user_email }}</td>
                </tr>
            {{/each}}
            </tbody>
        </table>
    </div>
</div>
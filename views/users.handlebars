<nav>
    <ul>
        <li class="btn red brandLogo border"><a href="/about">Chuck's Tees</a></li>
        <li class="border"><a href="/adminLanding">Admin Home</a></li>
        <li class="border"><a href="/users">Users</a></li>
        <li class="border"><a href="/inventory">Inventory</a></li>
